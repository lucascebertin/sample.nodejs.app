{
  "name": "nodejs-startup",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "bluebird": "^3.5.1",
    "debug": "^3.1.0",
    "rimraf": "^2.6.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^22.4.1",
    "babel-plugin-transform-async-to-bluebird": "^1.1.1",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-es2015-for-of": "^6.23.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-es2015-spread": "^6.22.0",
    "babel-plugin-transform-exponentiation-operator": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.18.1",
    "eslint-plugin-jsdoc": "3.3.1",
    "jest": "^22.4.2",
    "regenerator-runtime": "^0.11.1"
  },
  "scripts": {
    "clean": "rimraf {cache,coverage,dist}",
    "lint": "eslint .",
    "watch": "babel --watch src -d dist --ignore **/*.spec.js",
    "build": "yarn clean && npm run lint && babel src -d dist --ignore **/*.spec.js",
    "build:dev": "yarn clean && npm run lint && babel src -d dist --ignore **/*.spec.js --source-maps inline",
    "test": "jest --config jest.config.js --forceExit"
  }
}
